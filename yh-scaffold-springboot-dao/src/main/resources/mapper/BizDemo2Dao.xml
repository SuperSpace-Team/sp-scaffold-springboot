<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yh.scaffold.springboot.dao.repo.role.BizDemo2Dao">

    <!--[示例][本文件初版代码可通过代码生成器生成]-->
    <resultMap id="TestInfoMap" type="com.yh.scaffold.springboot.dao.po.DemoBizPO">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="version" property="version" />
        <result column="code" property="code" />
        <result column="name" property="name" />
        <result column="description" property="description" />
        <result column="is_delete" property="delete" />
        <result column="created_by" property="createdBy" />
        <result column="created_at" property="createdAt" />
        <result column="updated_by" property="updatedBy" />
        <result column="updated_at" property="updatedAt" />
    </resultMap>

    <select id="getTestDataList" resultMap="TestInfoMap">
        select code from test_1
    </select>

    <select id="findListByQueryMapWithPage" resultMap="TestInfoMap">
        select
        <include refid="columnResult"/>
        from test_1
    </select>

    <select id="findListCountByQueryMap" resultType="java.lang.Integer">
        select count(id)
        from test_1
        <include refid="findListWithPageWhere"/>
    </select>

    <sql id="columnResult">
        <![CDATA[
            id,
            version,
            code,
            `name`,
            description,
            is_delete,
            created_by,
            updated_by,
            created_at,
            updated_at
        ]]>
    </sql>

    <sql id="findListWithPageWhere">
        <where>
            <if test="id!=null">
                and id = #{id}
            </if>
            <if test="code!=null">
                and code = #{code}
            </if>
            <if test="version != null">
                version,
            </if>
            <if test="isDeleted!=null">
                and is_deleted = #{isDeleted}
            </if>
            <if test="searchParamLike != null and searchParamLike!=''">
                and `name` like concat('%',#{searchParamLike},'%')
            </if>
        </where>
    </sql>

</mapper>